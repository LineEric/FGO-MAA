{
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        },
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": ".*",
                "window_regex": "Visual Studio"
            }
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        },
        {
            "name": "B 服",
            "path": [
                "{PROJECT_DIR}/resource",
                "{PROJECT_DIR}/resource_bilibili"
            ]
        }
    ],
    "agent": {
        "child_exec": "python",
        "child_args": [
            "{PROJECT_DIR}/../agent/main.py"
        ]
    },
    "task": [
        {
            "name": "启动游戏",
            "entry": "FGO-B服登录"
        },
        {
            "name": "退出游戏",
            "entry": "FGO-B服退出",
            "option": [
                "任务选项1"
            ]
        },
        {
            "name": "自动战斗",
            "entry": "FGO自动战斗",
            "option": [
                "任务选项1"
            ],
            "pipeline_override": {}
        }
    ],
    "option": {
        "任务选项1": {
            "cases": []
        }
    }
}